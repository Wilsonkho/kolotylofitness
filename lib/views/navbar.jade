nav.navbar.navbar-default.navbar-fixed-top
  - var nav = {}; nav[title] = 'active'
  .container
    .navbar-header
      button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar-collapse')
        span.sr-only Toggle navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar
      a.navbar-brand(href='/') #{siteName}
    #navbar-collapse.collapse.navbar-collapse
      ul.nav.navbar-nav
        li(class='#{nav.Home}')
          a(href='/') Home
        li(class='#{nav.Packages}')
          a(href='/packages') Packages
        li(class='#{nav.About}')
          a(href='/about') About
      ul.nav.navbar-nav.navbar-right
        li(class='#{nav.instagram}')
          a(href='') 
            img(src="/images/icons/Instagram.png" alt="instagram" class="icon")
        li(class='#{nav.facebook}')
          a(href='') 
            img(src="/images/icons/Facebook.png" alt="facebook" class="icon")
        li(class='#{nav.youtube}')
          a(href='https://www.youtube.com/channel/UCHmXbbqcu1hXCRl4wawMwJA') 
            img(src="/images/icons/YouTube.png" alt="youtube" class="icon") 
